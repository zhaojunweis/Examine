<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.examine.dao.UserMapperTest" >

    <resultMap id="userResultMap" type="com.examine.domain.User">
        <id property="userId" column="user_id"/>
        <result property="userName" column="user_name"/>
        <result property="userPass" column="user_pass"/>
        <result property="userEmail" column="user_email"/>
        <result property="userProfilePhoto" column="user_profile_photo"/>
        <result property="userRegistrationTime" column="user_registration_time"/>
        <result property="userBirthday" column="user_birthday"/>
        <result property="userAge" column="user_age"/>
        <result property="userTelephoneNumber" column="user_telephone_number"/>
        <result property="userNickname" column="user_nickname"/>
    </resultMap>

    <select id = "selectAllUser" resultMap="userResultMap">
       select * from t_user
    </select>

</mapper>
